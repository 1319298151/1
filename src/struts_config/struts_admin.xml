<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="actionAdmin" namespace="/actionAdmin" extends="struts-default" >
	
		<action name="scheduleCoursePage" class="com.hbut.action.AdminAction" method="scheduleCoursePage">
			<result name="success">/pageAreas/adminPage/scheduleCoursePage.jsp</result>
		</action>
		
		<action name="schedulePage" class="com.hbut.action.AdminAction" method="schedulePage">
 			<result name="success">/pageAreas/adminPage/schedule.jsp</result>
		</action>
		
		<action name="submitSchedule" class="com.hbut.action.AdminAction" method="submitSchedule">
 			<result name="success">/pageAreas/adminPage/schedule.jsp</result>
		</action>
		
		<action name="updateSchedulePage" class="com.hbut.action.AdminAction" method="updateSchedulePage">
 			<result name="success">/pageAreas/adminPage/updateSchedulePage.jsp</result>
		</action>
		
		<action name="updateSchedule" class="com.hbut.action.AdminAction" method="updateSchedule">
 			<result name="success">/pageAreas/adminPage/updateSchedule.jsp</result>
		</action>
		
		<action name="update" class="com.hbut.action.AdminAction" method="update">
 			<result name="success">/pageAreas/adminPage/updateSchedule.jsp</result>
		</action>
		
	</package>

</struts>
