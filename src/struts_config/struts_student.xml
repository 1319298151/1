<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="actionStudent" namespace="/actionStudent" extends="struts-default" >
	
		<action name="showSchedule" class="com.hbut.action.StudentAction" method="showSchedule">
			<result name="success">/pageAreas/studentPage/showSchedule.jsp</result>
		</action>
		
		<action name="searchEmptyRoom" class="com.hbut.action.StudentAction" method="searchEmptyRoom">
			<result name="success">/pageAreas/searchRoom.jsp</result>
		</action>
		
		<action name="showResult" class="com.hbut.action.RoomAction" method="showResult">
			<result name="success">/pageAreas/showRoomResult.jsp</result>
		</action>
	</package>

</struts>
